{
  "name": "bugover/laravel-repository",
  "description": "repository abstract layer for eloquent with tagging cache",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "repository-eloquent",
  "version": "1.3.5",
  "authors": [
    {
      "name": "artak",
      "email": "manukyan.artak.21@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2.1 || <=8.3",
    "php-64bit": ">=8.2.1 || <=8.3",
    "ext-pdo": "*",
    "ext-json": "*",
    "illuminate/database": "^9.0|^10.0",
    "illuminate/contracts": "^9.0|^10.0",
    "illuminate/support": "^9.0|^10.0",
    "illuminate/pagination": "^9.0|^10.0",
    "illuminate/http": "^9.0|^10.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "phpunit/phpunit": "^9.6.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Service\\Repository\\RepositoryServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Service\\Repository\\": "src/"
    }
  }
}
